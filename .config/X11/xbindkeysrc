"dropdown-terminal" # quake terminal
    Mod4 + q

"launch" # program launcher
    Mod4 + r

"launch privilege" # privilege launcher
    Mod4 + e

"launch terminal" # terminal launcher
    Mod4 + w

"sh -c 'notify-send Zoom && boomer'" # zoom application
    Mod4 + x

"wmctrl -c :ACTIVE:" # close active window
    Mod4 + Shift + c

"wmctrl -r :ACTIVE: -b toggle,above" # toggle ontop active window
    Mod4 + t

"bookmarks show $HOME/Shares/Projects/bookmarks/archives/bookmarks-old.html" # show bookmarks
    Mod4 + g

"clipboard" # clipboard toggle
    Mod4 + z

"plumber" # text plumber
    Mod4 + i

"pass-menu" # pass menu
    Mod4 + d

"file-search" # file search
    Mod4 + p

"window-switcher" # window switcher
    Alt + Tab

"window-overview" # window overview
    Mod4 + Tab

"internet-search" # file search
    Mod4 + a

"volume-control up" # volume up
    XF86AudioRaiseVolume

"volume-control down" # volume down
    XF86AudioLowerVolume

"volume-control mute" # volume mute
    XF86AudioMute

"lockscreen" # lock screen
    XF86ScreenSaver

"snipping-tool" # snipping tool
    Print

"switch-monitor" # switch monitor
    XF86Display

"peek" # screen record
    Alt + Print

"flameshot gui" # screenshots
    Control + Print

"urxvt -hold -e xbindkeys --multikey -f $HOME/.config/X11/xbindkeysrc" # shortcut key check
    Shift + Mod4 + b

"urxvt" # terminal
    Mod4 + Return

"urxvt -pe tabbed" # terminal tabbed
    Control + Mod4 + Return

"urxvt -e tmux attach" # terminal multiplexer
    Alt + Mod4 + Return
